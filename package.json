{
  "name": "masterbot",
  "version": "0.8.0",
  "description": "A master bot for polybot",
  "engines": {
    "node": "7.4.0"
  },
  "main": "index.js",
  "scripts": {
    "test": "xo",
    "start": "./node_modules/.bin/gulp && node dist",
    "now-build": "gulp",
    "now-start": "node dist"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:polybot/polybot-api.git"
  },
  "babel": {
    "comments": false,
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime",
      "transform-async-to-generator"
    ]
  },
  "xo": {
    "esnext": true,
    "space": true,
    "semicolon": false,
    "ignores": [
      "dist/**"
    ],
    "rules": {
      "import/no-unresolved": 0,
      "import/no-dynamic-require": 0
    }
  },
  "keywords": [
    "polybot",
    "chatbot",
    "messaging",
    "ai",
    "slack",
    "telegram",
    "messenger"
  ],
  "author": "Mario Colque <colquemario@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.22.0",
    "bluebird": "^3.4.7",
    "botkit": "^0.4.0",
    "gulp": "^3.9.1",
    "request": "^2.76.0",
    "telebot": "^1.0.7",
    "winston": "^2.3.1",
    "xo": "^0.17.1"
  }
}
